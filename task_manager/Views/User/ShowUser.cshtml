@{
    ViewBag.Title = "Title";

    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}
<div class="container mt-5">
    <div class="d-flex justify-content-center">
        @model task_manager.Models.User
        <div>
            <div class="d-flex justify-content-between">
                <h4 class="font-weight-bold">User @Model.UserId</h4>
                <a href="/User/Index" class="btn btn-success">Back</a>
            </div>
            <div class="jumbotron">
                <h1 class="display-4">@Model.FirstName @Model.LastName</h1>
                <hr class="my-4">
                <p>Username: @Model.Username</p>
                <p>Password: @Model.Password</p>
                <p>Email: @Model.Email</p>
            </div>
        </div>
    </div>
</div>